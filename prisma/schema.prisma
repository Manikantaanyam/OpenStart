generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}



model Project{
  id Int @id @default(autoincrement())
  display_name String
  full_name String
  description String
  avatar_url String
  stars Int
  techStack String[]
  active DateTime
}